  },
  "bugs": {
    "url": "https://github.com/adamwong246/adamwong246.github.io/issues"
  },
  "homepage": "http://adamwong246.github.io/",
  "scripts": {
    "help": "cat help.txt",
    "webcrack:build": "node ./webcrack/index.js ../webcrack.config.js build",
    "webcrack:watch": "node ./webcrack/index.js ../webcrack.config.js watch",
    "webcrack:dev": "nodemon ./webcrack/index.js ../webcrack.config.js watch",
    "serve": "node ./server.js",
    "dev": "rm -rf dist/**; yarn serve & yarn webcrack:dev",
    "cleanBuild": "rm -rf dist/**; yarn webcrack:build",
    "newBlogEntry": "node newBlogEntry.js",
    "deploy": "git push origin `git subtree split --prefix dist dev`:master --force",
    "shipIt": "git add --all && git commit && yarn deploy"
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "buffer": "^5.5.0",
    "cherio": "^1.0.0-rc.2",
    "chokidar": "^3.3.1",
