doctype html
html(lang="en")
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name='HandheldFriendly', content='True')
    meta(name='MobileOptimized', content='320')
    meta(name='viewport', content='width=device-width, minimum-scale=1.0, maximum-scale=1.0')
    link(href='/style.css', rel='stylesheet', type="text/css")
    block pageTitle
      title exact change only
    script.
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-47887894-1', 'auto');
      ga('send', 'pageview');
  body
    main
      block content
        != page.content
    nav
      //- img#profile(src="/evilShroom.png")
      a(href="/") #[h1.site_title exact change only]

      a(href="mailto:" + package.author.email)= package.author.email
      hr
      a(href="/resume.html") resume.html
      |, 
      a(href="/resume.pdf") .pdf
      hr
      ul
        - for page in pages
          li #[a(href=page.url)= page.title]
      hr
      ul
        - for entry in blogEntries
          li #[a(href=entry.url)= entry.meta.title]
      
      
      a(href=package.repository.url) github
      br
      |v#{package.version}    
      br
      span #{moment().format()}
