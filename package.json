    "filepath": "^1.1.0",
    "fs-extra": "9.0.0",
    "glob": "latest",
    "glob-promise": "^4.0.1",
    "html-minifier": "",
    "jade": "1.11.0",
    "jpeg-js": "^0.3.7",
    "js-lwip": "^1.1.0",
    "jstransformer-marked": "^1.0.3",
    "lodash": "^2.4.1",
    "marky-mark": "latest",
    "memoizee": "^0.3.8",
    "mkdirp-then": "",
    "moment": "^2.9.0",
    "node-static": "^0.7.6",
    "nodemon": "^2.0.4",
    "normalize.css": "^3.0.2",
    "path": "^0.11.14",
    "puppeteer": "^3.0.1",
    "read-files-promise": "",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "simple-icons": "^2.7.0",
    "slug": "^0.8.0",
    "truncate-html": "^1.0.3",
    "yaml-front-matter": "^3.2.3"
  }
}
