  },
  "homepage": "http://adamwong246.github.io/",
  "type": "module",
  "scripts": {
    "funkophile:build": "node ./funkophile.config.js build",
    "funkophile:watch": "node ./funkophile.config.js watch",
    "funkophile:dev": "nodemon  ./funkophile.config.js watch",
    "serve": "node ./server.cjs",
    "dev": "yarn serve & yarn funkophile:dev",
    "cleanBuild": "rm -rf dist/**; yarn funkophile:build; yarn resume:pdf; yarn resume:html",
    "newBlogEntry": "node newBlogEntry.js",
    "deploy": "git push origin `git subtree split --prefix dist dev`:master --force",
    "shipIt!": "yarn cleanBuild && git add --all && git push origin dev && git commit && yarn deploy",
    "resume:pdf": "pandoc src/resume.rmd -o dist/resume.pdf -t html --self-contained --css dist/resume.pdf.css",
    "resume:html": "pandoc -t html --self-contained --css=dist/resume.html.css -o dist/resume.html src/resume.rmd"

  },
  "dependencies": {
    "@babel/core": "^7.12.13",
